{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
    },
    "rules": {
    "no-unused-vars": "warn",
    "no-restricted-syntax": "off",
    "prettier/prettier": ["error"],
    "import/prefer-default-export": "off",
    "import/extensions": "off"
    },
    "plugins": ["prettier"]
}
